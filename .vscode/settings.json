{
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.colorDecorators": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnPaste": false,
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file",
        "editor.formatOnType": true,
        "editor.parameterHints.cycle": true,
        "editor.snippetSuggestions": "top",
        "editor.suggest.preview": true,
        "editor.trimAutoWhitespace": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "better-comments.tags": [
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#FF0000",
            "italic": false,
            "strikethrough": false,
            "tag": "!",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": true,
            "color": "#00ffff",
            "italic": false,
            "strikethrough": false,
            "tag": "?",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#114488",
            "italic": false,
            "strikethrough": false,
            "tag": "#",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": true,
            "color": "#114488",
            "italic": false,
            "strikethrough": false,
            "tag": "##",
            "underline": false
        }
    ],
    "cSpell.customDictionaries": {
        "custom": true,
        "internal-terms": true,
        "project-words": {
            "addWords": true,
            "description": "Words that Max uses a lot.",
            "name": "Max's Dictionary",
            "path": "/Users/maxludden/.cspell/maxDictionary.txt"
        },
        "python": true
    },
    "cSpell.enableFiletypes": [
        "dotenv",
        "gitignore",
        "kivy",
        "log",
        "rainbow hover markup",
        "shellscript",
        "sqlite",
        "ssh_config",
        "toml",
        "vb",
        "python"
    ],
    "cSpell.ignoreWords": [
        "ARGB",
        "HEXA",
        "Ludden",
        "Meslo",
        "Scrollback",
        "Sysnative",
        "aaffaa",
        "docsmsft",
        "iusmod",
        "mclass",
        "nchr",
        "pyenv",
        "pylint",
        "stepsize",
        "twotwelve",
        "windir"
    ],
    "cSpell.userWords": [
        "greylock",
        "maxbook",
        "maxludden",
        "TODOC",
        "venvs",
        "webcolors"
    ],
    "cSpell.words": [
        "Donath",
        "maxgradient",
        "maxlog"
    ],
    "colorize.colorized_variables": [
        "LESS",
        "CSS",
        "SASS",
        "STYLUS"
    ],
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {
        "editor.action.formatDocument": "explicit",
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.colorDecoratorsLimit": 800,
    "editor.fontFamily": "MesloLGS NF, Menlo, Monaco, 'Courier New', monospace",
    "editor.inlineSuggest.enabled": true,
    "editor.linkedEditing": true,
    "editor.semanticTokenColorCustomizations": {},
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "string.quoted.double.json meta.structure.dictionary.json string.quoted.double.json",
                "settings": {
                    "foreground": "#00FF00"
                }
            }
        ]
    },
    "errorLens.excludeBySource": [
        "Pylint(F0002:astroid-error)"
    ],
    "errorLens.lintFilePaths": {
        "Stylelint": [
            "**/*.stylelintrc",
            "**/*.stylelintrc.{cjs,js,json,yaml,yml}",
            "**/*stylelint.config.{cjs,js}",
            "**/*package.json"
        ],
        "eslint": [
            "**/*.eslintrc.{js,cjs,yaml,yml,json}",
            "**/*package.json"
        ]
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "__pychache__": true
    },
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.ignoredRepositories": [
        "/Users/maxludden/.vscode/extensions/stepsize.stepsize-1.0.15/resources/demo-repository"
    ],
    "github-actions.workflows.pinned.workflows": [
        ".github/workflows/jekyll-gh-pages.yml"
    ],
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": false,
        "python": true,
        "scminput": false,
        "yaml": false
    },
    "markdown.styles": [
        "styles/gradient.css"
    ],
    "markdownlint.config": {
        "MD003": {
            "style": "atx"
        },
        "MD007": {
            "indent": 4
        },
        "MD013": false,
        "MD033": false,
        "MD041": false,
        "default": true,
        "no-hard-tabs": false
    },
    "projectManager.git.baseFolders": [
        "max",
        "/Users/maxludden/dev/py"
    ],
    "python.defaultInterpreterPath": "/Users/maxludden/.config/.pyenv/versions/3.11.5/bin/python3.11",
    "python.testing.pytestEnabled": false,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./tests",
        "-p",
        "*test*.py"
    ],
    "python.testing.unittestEnabled": true,
    "svg.preview.background": "editor",
    "syntax.highlightLanguages": [
        "html",
        "c",
        "cpp",
        "python",
        "typescript",
        "typescriptreact",
        "javascript",
        "go",
        "rust",
        "php",
        "ruby",
        "shellscript",
        "ocaml",
        "lua"
